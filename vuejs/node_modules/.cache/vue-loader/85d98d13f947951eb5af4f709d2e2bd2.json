:["<template>\n  <div class=\"users\">\n    <div\n      v-if=\"loading\"\n      class=\"loading\"\n    >\n      Merci de patientre...\n    </div>\n    <div\n      v-if=\"error\"\n      class=\"error\"\n    >\n      {{ error }}\n    </div>\n    <v-container v-if=\"users!=null\">\n      {{ users }}\n    </v-container>\n  </div>\n</template>\n\n<script>\n//import { storage, Storage } from \"@/storage\";\nimport store from \"@/store\";\nimport EventBus from \"@/eventBus\";\nexport default {\n  name: \"Application\",\n  mounted () {\n    this.$on('users:loaded', this.fetchData())\n  },\n  created () {\n      store.dispatch('loadUsers');\n  },\n  computed: {\n    user: {\n      get() {\n        var u = store.state.user;\n        if(u!=null && user.authorities.find(e=>e.authorities='ROLE_ADMIN')){\n          return store.state.user;\n        }\n        return null;\n    },\n    users: {\n      get() {\n        return store.state.users;\n      }\n    },\n  },\n  props: {},\n  data() {\n    return {\n      loading: false,\n      error: null,\n    };\n  },\n  methods: {\n    fetchData () {\n      this.error = this.users = null;\n      this.loading = true;\n    }\n  },\n  components: {\n    //Configuration,\n  },\n  watch: {\n    '$route': 'fetchData'\n  },\n}\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"]}]}